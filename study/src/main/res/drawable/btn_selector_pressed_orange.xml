<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <item
        android:state_pressed="true">

        <shape>
            <solid
                android:color="#975C28"/>
            <corners
                android:radius="7dp"/>
        </shape>


    </item>

    <item
        android:state_pressed="false">

        <shape>
            <stroke
                android:color="#975C28"/>
            <corners
                android:radius="7dp"/>
        </shape>

    </item>


</selector>